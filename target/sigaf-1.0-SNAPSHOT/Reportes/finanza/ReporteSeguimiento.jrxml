     t_proyecto."plazo" AS t_proyecto_plazo,
     t_proyecto."fecha" AS t_proyecto_fecha,
     t_proyecto."estado" AS t_proyecto_estado,
     t_proyecto."idtipocredito" AS t_proyecto_idtipocredito,
